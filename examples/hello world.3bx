type:
	#when referenced as a type
	pattern:
		number
	
effect:
	pattern:
		set x to y
	when executed:
		@intrinsic("store", x, y)

class:
	pattern:
		vector
	members:
		x, y, z
	when created:
		set each member to 0
	add:
		add each member of the adder to each member of me
	remove:
		remove each member of the adder from each member of me



